<resources>
    <declare-styleable name="A1Taxi">
        <attr name="circleShapeBackground" format="reference" />
        <attr name="textColorWhiteSelector" format="reference" />
        <attr name="buttonSelectorGreen" format="reference" />
        <attr name="buttonSelectorRed" format="reference" />
    </declare-styleable>

    <declare-styleable name="MaskedEditText">
        <attr name="mask" format="string" />
        <attr name="allowed_chars" format="string" />
        <attr name="denied_chars" format="string" />
        <attr name="char_representation" format="string" />
        <attr name="keep_hint" format="boolean" />
    </declare-styleable>

    <declare-styleable name="RippleText">
        <attr name="rippleEnabled" format="boolean" />
        <attr name="backgroundColor" format="color" />
        <attr name="rippleColor" format="color" />
    </declare-styleable>
</resources>